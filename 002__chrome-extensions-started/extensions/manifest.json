{
  "name": "Gray Webpage",
  "description": "使网站变灰",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },

  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}